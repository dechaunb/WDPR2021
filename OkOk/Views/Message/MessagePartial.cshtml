@model Message
<li class="d-flex align-items-center justify-content-between">
    <p class="list-group-item w-75 m-2">
        @Model.Sender.FirstName zei om @Model.DateTime: "@Model.Content" 
    </p>
    <button class="report flex-end align-items-center btn" onclick="report('@Model.Id')">
        <i class="fa-solid fa-triangle-exclamation text-primary"></i>
    </button>
</li>